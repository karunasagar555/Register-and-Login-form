    private void button1ActionPerformed(java.awt.event.ActionEvent evt) {                                        
    try
        {
            Class.forName("com.mysql.jdbc.Driver");
            Connection con = DriverManager.getConnection("jdbc:mysql://localhost:3306/database1","root","");
            String sql = "Select * from Main_Table where username=? and password=?";
            PreparedStatement pst=con.prepareStatement(sql);
            pst.setString(1,username.getText());
            pst.setString(2,password.getText());
            ResultSet rs= pst.executeQuery();
            if(rs.next())
            {
                JOptionPane.showMessageDialog(null, "Username and Password are correct. Login Successfull");
            }
            else
            {
                JOptionPane.showMessageDialog(null, "Username and Password are  incorrect. Login Unsuccessfull");
                username.setText("");
                password.setText("");
            }
            con.close();
        }
        catch(Exception e)
        {
            JOptionPane.showMessageDialog(null,e);
        }        // TODO add your handling code here:
    }                                       

